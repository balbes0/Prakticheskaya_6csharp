<Window x:Class="Prakticheskaya_6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prakticheskaya_6"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="80*"/>
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="15*"/>
                <ColumnDefinition Width="70*"/>
                <ColumnDefinition Width="15*"/>
            </Grid.ColumnDefinitions>
            <Button x:Name="BeforeDateButton" Click="BeforeDateButton_Click">НАЗАД</Button>
            <Button x:Name="AfterDateButton" Grid.Column="2" Click="AfterDateButton_Click">ВПЕРЕД</Button>
            <Label x:Name="DateNowLabel" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20">ТЕКУЩАЯ ДАТА</Label>
        </Grid>
        <Separator Grid.Row="1" VerticalAlignment="Top"/>
        <Grid Grid.Row="1">
            <ScrollViewer>
                <WrapPanel x:Name="wrapPanel">
                    
                </WrapPanel>
            </ScrollViewer>
        </Grid>
    </Grid>
</Window>
